/dts-v1/;
#include "imx6q.dtsi"
#include "imx6qdl-loonswitch.dtsi"
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "CelerityEDS iMX6COM Board";
	compatible = "fsl,imx6q";

	leds {
		compatible = "gpio-leds";
		cpuact {
			label = "cpu_activity";
			gpios = <&gpio4 26 1>;
			linux,default-trigger = "heartbeat";
			default-state = "on";
		};
		user0 {
			label = "user0";
			gpios = <&gpio4 28 1>;
			default-state = "on";
		};
		user1 {
			label = "user1";
			gpios = <&gpio4 27 1>;
			default-state = "on";
		};
	};
};